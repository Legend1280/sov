{
  "id": "dexabooks_main",
  "name": "DexaBooks Main Viewport",
  "app": "dexabooks",
  "viewports": {
    "viewport1": [
      {
        "type": "Stack",
        "props": {
          "direction": "vertical",
          "gap": 6
        },
        "children": [
          {
            "type": "Text",
            "props": {
              "text": "DexaBooks Dashboard",
              "as": "h1",
              "className": "text-3xl font-bold"
            }
          },
          {
            "type": "Grid",
            "props": {
              "columns": 2,
              "gap": 4
            },
            "children": [
              {
                "type": "Text",
                "props": {
                  "text": "Financial analytics and transaction management will be displayed here.",
                  "className": "text-muted-foreground"
                }
              },
              {
                "type": "Text",
                "props": {
                  "text": "Connect your DexaBooks module components to populate this viewport.",
                  "className": "text-muted-foreground"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
