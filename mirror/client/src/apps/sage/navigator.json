{
  "id": "sage_navigator",
  "name": "SAGE Navigator",
  "app": "sage",
  "viewports": {
    "viewport1": [
      {
        "type": "Stack",
        "props": {
          "direction": "vertical",
          "gap": 2
        },
        "children": [
          {
            "type": "NavigatorSection",
            "props": {
              "title": "GOVERNANCE"
            },
            "children": [
              {
                "type": "NavigatorItem",
                "props": {
                  "id": "pending_validations",
                  "label": "Pending Validations",
                  "icon": "AlertCircle",
                  "badge": "{{pendingCount}}"
                }
              },
              {
                "type": "NavigatorItem",
                "props": {
                  "id": "recent_decisions",
                  "label": "Recent Decisions",
                  "icon": "CheckCircle"
                }
              },
              {
                "type": "NavigatorItem",
                "props": {
                  "id": "coherence_monitor",
                  "label": "Coherence Monitor",
                  "icon": "Activity"
                }
              },
              {
                "type": "NavigatorItem",
                "props": {
                  "id": "trust_scores",
                  "label": "Trust Scores",
                  "icon": "Shield"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}
